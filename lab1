#include <stdio.h>
#include <math.h>
int main()
{
    int i;
    double a, b, c;
    float a1;
    double d;
    double x1, x2;
    a=5;
    b=1; 
    c=-2; 
    printf("Wedite a,b,c:");
    scanf("%lf",&a);
    scanf("%lf",&b);
    scanf("%lf",&c);
    d = pow(b,2) - 4*a*c;
    
    
    printf("d = %lf \n",d);
    if(d>0){
    x1 = (-b - sqrt(d)/(2*a));
    x2 = (-b + sqrt(d)/(2*a));
    printf("x1 = %lf x2 = %lf", x1,x2);
    }
    if(d<0){
    printf("Kornei net");
    }
    if(d==0){
    printf("Odin koren");
    x1 = (-b - sqrt(d)/(2*a));
    printf("x1 = %lf",x1);
    }
    return 0;
}
